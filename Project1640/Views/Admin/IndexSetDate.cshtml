
@model IEnumerable<Project1640.Models.SetDate>

@{
    Layout = "~/Views/Admin/_LayoutAdmin.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table class="table table-hover">
        <thead class="thead-dark">
            <tr>      
                <th scope="col">@Html.DisplayNameFor(model => model.StartDate)</th>
                <th scope="col">@Html.DisplayNameFor(model => model.EndDate)</th>
                <th scope="col">Action</th>
            </tr>
        </thead>


        <tbody>
            @foreach (var c in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(model => c.StartDate)</td>
                    <td>@Html.DisplayFor(model => c.EndDate)</td>
                    <td>
                        <a type="button" class="btn btn-success btn-sm"
                           href="@Url.Action("EditDate", new {id = c.Id})">Edit</a>
                        <a type="button" class="btn btn-danger btn-sm"
                           href="@Url.Action("DeleteDate", new {id = c.Id })">Delete</a>

                    </td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>


